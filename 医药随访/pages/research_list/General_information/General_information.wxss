/* pages/research_list/General_information/General_information.wxss */

.title{
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  color: #397CDA;
  background-color: #F8F8F8;
  border-bottom: 1rpx solid #ECECEC;
}
.list{
  height: 90rpx;
  line-height: 90rpx;
  background-color: white;
  color: #919191;
  display: flex;
  flex-direction: row;
  font-size: 30rpx;
  width: 100%;
  border-bottom: 1rpx solid #ECECEC;
}
.name{
  margin-left: 20rpx;
}
.text{
  margin-left: 20rpx;
  color: #555555;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  /* border: 1rpx solid black */
}
.button{
  background-color: #013F91;
  width: 100%;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  color: white;
  font-size: 32rpx;
  position: fixed;
  bottom: 0;
}