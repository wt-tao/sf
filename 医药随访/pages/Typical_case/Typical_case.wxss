/* pages/Typical_case/Typical_case.wxss */
page{
  color: #979797;
  font-size: 30rpx;
}
.div{
   width: 100%;
   margin-top: 30rpx;
   background-color: white;
}
.list{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  flex-direction: row;
  border-bottom: 1rpx solid #DDDDDD;
}
.name{
  margin-left: 20rpx;
  width: 25%;
  
}
.input{
  height: 80rpx;
  /* border: 1rpx solid black; */
}
.names{
  margin-left: 20rpx;
  color: #131313;
}
.liad_addr{
  width: 33.3%;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  border: 1rpx solid black;
}
.days{
  margin-left: 20rpx;
  width: 60%;
  border: 1rpx solid black;
}
.list_city{
  width: 100%;
  display: flex;
  flex-direction: row;
  border-left: 1rpx solid #EBEBEB;
  font-size: 30rpx;
  color: #949494;
  line-height: 90rpx;
}
.list_city_text{
  width: 75%;
  margin-left: 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
   /* border: 1rpx solid black; */
}
.textarea{
  width: 96%;
  height: 250rpx;
  margin-top: 10rpx;
  padding-left: 20rpx;
  /* border: 1rpx solid black; */
}
.picker{
  width: 100%;
  display: flex;
  flex-direction: row;
  border-right: 1rpx solid #E6E6E6;
  border-bottom: 1rpx solid #E6E6E6
}
.text{
  text-align: right;
  width: 200rpx;
}
.divs{
  width: 100%;
  background-color: white;
  border-bottom: 1rpx solid #DDDDDD;
}
.title1{
  height:90rpx;
  line-height: 90rpx;
  margin-left: 20rpx;
  background-color: #F8F8F8;
  border-bottom: 1rpx solid #EEEEEE;
  color: #131313;
}
.photo{
  width: 97.5%;
  min-height: 250rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  /* border: 1rpx solid black; */
}
.photo_img{
  height: 125rpx;
  width: 125rpx;
  background-color: #D3D3D3;
  margin-top: 20rpx;
  margin-left: 2.5%;
}