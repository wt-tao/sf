/**app.wxss**/
page{
  background-color: #EBEBEB
}
.upload{
  width: 100%;
  height: 100rpx;
  background-color: #013F91;
  font-size: 30rpx;
  color: white;
  line-height: 100rpx;
  text-align: center;
  position: fixed;
  bottom: 0;
}

.button{
  background-color: #013F91;
  width: 100%;
  text-align: center;
  margin-top: 30rpx;
  height: 100rpx;
  line-height: 100rpx;
  color: white;
  font-size: 32rpx;
}
.fixed{
  position: fixed;
  bottom: 0;
}
.title{
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  color: #397CDA;
  background-color: #F8F8F8;
  border-bottom: 1rpx solid #ECECEC;
}
.row{
  display: flex;
  flex-direction: row;
}

.uploads{
  width: 190rpx;
  margin-left: 30rpx;
  border: 1rpx solid #E5E5E5;
  height: 60rpx;
  margin-top: 20rpx;
  display: flex;
  flex-direction: row;
  background-color: #FFFFFF;
  border-radius: 30rpx;
  font-size: 28rpx;
  line-height: 60rpx;
  color: #888888;
}